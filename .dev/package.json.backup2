    "errorhandler": "^1.5.0",
    "express": "^4.14.0",
    "express-ejs-layouts": "^2.2.0",
    "express-session": "^1.14.2",
    "lodash": "^4.17.4",
    "method-override": "2.3.x",
    "moment": "^2.17.1",
    "ng-table": "^3.0.1",
